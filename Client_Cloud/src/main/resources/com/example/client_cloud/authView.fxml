<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="333.0" prefWidth="368.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client_cloud.AuthController">
    <Pane prefHeight="333.0" prefWidth="368.0">
        <children>
            <Button layoutX="131.0" layoutY="267.0" mnemonicParsing="false" onAction="#log_in" prefHeight="30.0" prefWidth="106.0" text="sign in" textAlignment="CENTER">
            <font>
               <Font name="Kinnari" size="20.0" />
            </font></Button>
            <TextField fx:id="login_in" layoutX="151.0" layoutY="131.0" prefHeight="24.0" prefWidth="170.0" promptText="enter login" />
            <TextField fx:id="password_in" layoutX="151.0" layoutY="196.0" promptText="enter password" />
            <Label layoutX="49.0" layoutY="134.0" prefHeight="15.0" prefWidth="88.0" text="Login">
            <font>
               <Font name="Kinnari" size="20.0" />
            </font></Label>
            <Label layoutX="49.0" layoutY="201.0" text="Password">
            <font>
               <Font name="Kinnari" size="20.0" />
            </font></Label>
         <Button fx:id="backButton" layoutX="289.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToStartView" prefHeight="24.0" prefWidth="65.0" text="Back" />
         <Label fx:id="welcome" layoutX="93.0" layoutY="55.0" text="WELCOME" />
        </children>
    </Pane>
</AnchorPane>
