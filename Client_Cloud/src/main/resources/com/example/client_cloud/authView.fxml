<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="239.0" prefWidth="199.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client_cloud.AuthController">
    <Pane prefHeight="239.0" prefWidth="199.0">
        <children>
            <Button layoutX="39.0" layoutY="172.0" mnemonicParsing="false" onAction="#log_in" prefHeight="38.0" prefWidth="105.0" text="sign in" textAlignment="CENTER">
            <font>
               <Font name="Kinnari" size="20.0" />
            </font></Button>
            <TextField fx:id="login_in" layoutX="15.0" layoutY="60.0" promptText="enter login" />
            <TextField fx:id="password_in" layoutX="15.0" layoutY="132.0" promptText="enter password" />
            <Label layoutX="15.0" layoutY="18.0" prefHeight="32.0" prefWidth="88.0" text="Login">
            <font>
               <Font name="Kinnari" size="20.0" />
            </font></Label>
            <Label layoutX="14.0" layoutY="93.0" text="Password">
            <font>
               <Font name="Kinnari" size="20.0" />
            </font></Label>
         <Button layoutX="140.0" layoutY="6.0" mnemonicParsing="false" onAction="#backToStartView" prefHeight="24.0" prefWidth="52.0" text="Back" />
        </children>
    </Pane>
</AnchorPane>
